<% @contacts.each do |contact| %>
  <div class="panel panel-primary">
    <div class="panel-heading">
      <h3 class="panel-title"><%= contact.name %></h3>
    </div>
    <div class="panel-body">
       <p><%= contact.email %></p>
       <p><%= simple_format(contact.message) %></p>
    </div>
  </div>
<% end %>